net add loopback lo ip address 10.19.241.12/32
net add bgp autonomous-system 65201
net add routing prefix-list ipv4 list1 seq 10 permit 10.19.241.0/24 ge 32
net add routing prefix-list ipv4 list1 seq 20 permit 192.168.0.0/16 ge 24
net add routing prefix-list ipv4 list1 seq 30 deny any
net add interface swp25 ip address 172.16.0.0/31
net add bgp network 10.19.241.12/32
net add bgp neighbor 172.16.0.1 remote-as 65001
net add bgp neighbor SPINES peer-group
net add bgp neighbor SPINES remote-as external
net add bgp neighbor SPINES capability extended-nexthop
net add bgp neighbor SPINES description Internal Fabric Network
net add bgp neighbor swp29-30 interface peer-group SPINES
net add bgp ipv4 unicast neighbor SPINES prefix-list list1 out
net add bgp bestpath as-path multipath-relax
net add vrf tenant1
net add interface swp25 vrf tenant1
net add bgp vrf tenant1 autonomous-system 65201
net add bgp vrf tenant1 bestpath as-path multipath-relax
net add bgp vrf tenant1 neighbor 172.16.0.1 remote-as 65001
net add bgp vrf tenant1 evpn advertise ipv4 unicast
net add vlan 4001 vrf tenant1
net add vlan 4001 hwaddress 44:38:39:11:00:15
net add bgp network 172.16.0.0/31 vrf tenant1
net add vxlan vni104001 vxlan id 104001
net add vxlan vni104001 bridge access 4001
net add vxlan vni104001 bridge learning off
net add vxlan vni104001 bridge arp-nd-suppress on
net add vxlan vni104001 vxlan local-tunnelip 10.19.241.12
net add vrf tenant1 vni 104001
